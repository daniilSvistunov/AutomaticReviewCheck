{
  "name": "oktemplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open --mode development-local",
    "build:local": "tsc && vite build --mode development-local",
    "build:dev": "tsc && vite build --mode dev",
    "build:stage": "tsc && vite build --mode stage",
    "build:prod": "tsc && vite build",
    "serve": "vite preview --open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && npx prettier --write ./src/",
    "lint:report": "eslint . -f json -o reports/eslint-report.json",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest coverage.enabled=true --ui",
    "preview": "vite preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@azure/msal-browser": "^3.10.0",
